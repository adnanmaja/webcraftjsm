{
  "version": 2,
  "builds": [
    {
      "src": "backend/main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "15mb",
        "includeFiles": "backend/**",
        "cwd": "backend"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/backend/main.py"
    }
  ]
}
